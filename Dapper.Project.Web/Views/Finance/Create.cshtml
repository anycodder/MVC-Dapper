    <div class="form-group">
        <label asp-for="sales_titles_id" class="control-label"></label>
        <input asp-for="sales_titles_id" class="form-control" />
        <span asp-validation-for="sales_titles_id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="suppliers_id" class="control-label"></label>
        <input asp-for="suppliers_id" class="form-control" />
        <span asp-validation-for="suppliers_id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="finance_date" class="control-label"></label>
        <input asp-for="finance_date" class="form-control" />
        <span asp-validation-for="finance_date" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="finance_earnings" class="control-label"></label>
        <input asp-for="finance_earnings" class="form-control" />
        <span asp-validation-for="finance_earnings" class="text-danger"></span>
    </div>
